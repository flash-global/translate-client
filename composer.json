{
  "name": "fei/translate-client",
  "description": "Flash applicative Translate - client component",
  "type": "project",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Yoctu",
      "email": "contact@yoctu.com",
      "homepage": "http://yoctu.com/"
    }
  ],
  "config": {
    "platform": {
      "php": "5.5"
    }
  },
  "autoload": {
    "psr-4": {
      "Fei\\Service\\Translate\\Client\\": "src/"
